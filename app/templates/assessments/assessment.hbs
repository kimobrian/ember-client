<h4>{{model.fellow}}'s Assessment Details</h4>
<ul class="list-group list-group-flush">
  <li class="list-group-item">
        <div class='flex-row'>
          <div class='flex-column-left'>Assessor</div>
          <div class='flex-column-right'>{{model.assessor}}</div>
        </div>
        <div class='flex-row'>
          <div class='flex-column-left'>Fellow</div>
          <div class='flex-column-right'>{{model.fellow}}</div>
        </div>
        <div class='flex-row'>
          <div class='flex-column-left'>Score</div>
          <div class='flex-column-right'>{{model.score}}%</div>
        </div>
        <div class='flex-row'>
          <div class='flex-column-left'>Status</div>
          <div class='flex-column-right'>{{model.status}}</div>
        </div>
      </li>
</ul>
